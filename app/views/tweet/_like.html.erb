  <% if Like.find_by(user_id: current_user.id, tweet_id: tweet.id) %>
    <%= link_to "/tweets/#{tweet.id}/like", method: :delete, local: true do %>
    <i class="material-icons">❤</i>
      <% end %>
  <% else %>
    <%= link_to "/tweets/#{tweet.id}/like", method: :post, local: true do %>
      <i class="material-icons">♡</i>
    <% end %>
  <% end %>
<%= tweet.likes.length %>